subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "ipv6",
    srcs = [
        "ipv6/batch.go",
        "ipv6/control.go",
        "ipv6/control_rfc3542_unix.go",
        "ipv6/control_unix.go",
        "ipv6/dgramopt.go",
        "ipv6/doc.go",
        "ipv6/endpoint.go",
        "ipv6/genericopt.go",
        "ipv6/header.go",
        "ipv6/helper.go",
        "ipv6/iana.go",
        "ipv6/icmp.go",
        "ipv6/payload.go",
        "ipv6/payload_cmsg.go",
        "ipv6/sockopt.go",
        "ipv6/sockopt_posix.go",
        "ipv6/sys_asmreq.go",
        "ipv6/sys_ssmreq.go",
    ] + select({
        "//third_party/go/__config:darwin_amd64": [
            "ipv6/control_rfc2292_unix.go",
            "ipv6/icmp_bsd.go",
            "ipv6/sys_bpf_stub.go",
            "ipv6/sys_darwin.go",
            "ipv6/zsys_darwin.go",
        ],
        "//third_party/go/__config:linux_amd64": [
            "ipv6/icmp_linux.go",
            "ipv6/sys_bpf.go",
            "ipv6/sys_linux.go",
            "ipv6/zsys_linux_amd64.go",
        ],
        "default": [],
    }),
    tag = "go_source",
    deps = ["//third_party/go/golang.org/x/net:_net#download"],
)

go_library(
    name = "ipv6",
    srcs = [":_ipv6#go_source"],
    cover = False,
    import_path = "golang.org/x/net/ipv6",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/golang.org/x/net/bpf",
        "//third_party/go/golang.org/x/net/internal/iana",
        "//third_party/go/golang.org/x/net/internal/socket",
    ] + select({
        "//third_party/go/__config:darwin_amd64": [],
        "//third_party/go/__config:linux_amd64": ["//third_party/go/golang.org/x/sys/unix"],
        "default": [],
    }),
)
